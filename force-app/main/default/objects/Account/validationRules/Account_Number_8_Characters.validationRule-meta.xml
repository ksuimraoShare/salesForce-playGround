<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_8_Characters</fullName>
    <active>false</active>
    <errorConditionFormula>LEN( AccountNumber) != 8</errorConditionFormula>
    <errorMessage>取引先番号は 8 文字にする必要があります。</errorMessage>
</ValidationRule>
